@using SysInternshipManagement.Models.Widget
@{
    Layout = "~/Views/Stage/_Layout.cshtml";
}

<div>
    <h1>Entreprise</h1>
    <div class="form-content">
        <div>
            @Html.Partial("Widget/_FlatUiSelect", new FlatUiSelect("contact"))
        </div>
        <div>
            @Html.Partial("Widget/_FlatUiTextInput", new FlatUiTextInputModel("Nom d'entreprise"))
